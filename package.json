{
  "name": "mvc.ts",
  "version": "0.1.0",
  "description": "A simple todo MVC app written in TypeScript",
  "author": "David J. Valles <davidtjvalles@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf dist .parcel-cache",
    "format": "prettier -w .",
    "lint": "xo",
    "dev": "parcel serve ./src/index.html",
    "build": "parcel build ./src/index.html",
    "deploy": "npm run clean && npm run build && gh-pages -d dist"
  },
  "devDependencies": {
    "@dtjv/tsconfig": "0.1.1",
    "@fontsource/inter": "4.1.0",
    "@parcel/config-default": "2.0.0-nightly.560",
    "@parcel/transformer-typescript-tsc": "2.0.0-nightly.560",
    "@parcel/validator-typescript": "2.0.0-nightly.560",
    "@tailwindcss/forms": "0.2.1",
    "autoprefixer": "10.2.4",
    "gh-pages": "3.1.0",
    "parcel": "2.0.0-nightly.558",
    "postcss": "8.2.4",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "tailwindcss": "2.0.2",
    "typescript": "4.1.3",
    "xo": "0.37.1"
  },
  "xo": {
    "prettier": true,
    "parserOptions": {
      "project": "./tsconfig.xo.json"
    }
  },
  "dependencies": {
    "cuid": "2.1.8"
  }
}
